@for (var i = 5; i > 0; i--)
{
    <div class="pull-left evalua-pull">
        <div class="pull-left evalua">
            <div style="height:9px;">@i <span class="glyphicon glyphicon-star"></span></div>
        </div>
        <div class="pull-left evalua-persent">
            <div class="progress" style="height:9px; margin:3px 0;">
                <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="5" aria-valuemin="0" aria-valuemax="5" style="width: @ViewBag.PercentStar[@i-1]%">
                    <span class="sr-only">@ViewBag.PercentStar[@i - 1]% Complete (danger)</span>
                </div>
            </div>
        </div>
        <div class="pull-right" style="margin-left:10px; display: inline-block;">@ViewBag.CountRating[@i - 1]</div>
    </div>
}
<input type="hidden" id="start-medium-val" value="@ViewBag.MediumStar" />